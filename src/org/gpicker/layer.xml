<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Edit">
            <file name="org-gpicker-actions-GPickFile.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.gpicker.Bundle"/>
                <attr name="delegate" newvalue="org.gpicker.actions.GPickFile"/>
                <attr name="displayName" bundlevalue="org.gpicker.actions.Bundle#CTL_GPickFile"/>
                <attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/>
                <attr name="noIconInMenu" boolvalue="false"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="GoTo">
            <file name="org-gpicker-actions-GPickFile.shadow">
                <attr name="originalFile" stringvalue="Actions/Edit/org-gpicker-actions-GPickFile.instance"/>
                <attr name="position" intvalue="0"/>
            </file>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="org-gpicker-settings-gpickerAdvancedOption.instance">
                <attr name="controller" newvalue="org.gpicker.settings.GPickerOptionsPanelController"/>
                <attr name="displayName" bundlevalue="org.gpicker.settings.Bundle#AdvancedOption_DisplayName_gpicker"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.options.AdvancedOption.createSubCategory"/>
                <attr name="keywords" bundlevalue="org.gpicker.settings.Bundle#AdvancedOption_Keywords_gpicker"/>
                <attr name="keywordsCategory" stringvalue="Advanced/null"/>
                <attr name="toolTip" bundlevalue="org.gpicker.settings.Bundle#AdvancedOption_Tooltip_gpicker"/>
            </file>
        </folder>
    </folder>
    <folder name="Shortcuts">
        <file name="DS-G.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-gpicker-actions-GPickFile.instance"/>
        </file>
    </folder>
</filesystem>
